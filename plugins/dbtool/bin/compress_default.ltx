[options] ; exclude files from compression with such extension
exclude_exts = *.ncb,*.sln,*.vcproj,*.old,*.rc,*.scc,*.vssscc,*.bmp,*.exe,*.db,*.bak*,*.bmp,*.smf,*.uvm,*.prj,*.tga,*.txt,*.rtf,*.doc,*.log,*.~*,*.rar,*.sfk

[include_folders] ; path to gamedata folders from current folder to compress
.\ = true         ; compress all files in gamedata - never use together with the below folders, or you compress those folders twice!
;anims = false
;configs = false
;levels = false
;meshes = false
;scripts = false
;shaders = false
;sounds = false
;spawns = false
;textures = false

[exclude_folders] ; exclude from compression the following folders of gamedata
;anims = false
;configs = false
;levels = false
;meshes = false
;scripts = false
;shaders = false
;sounds = false
;spawns = false
;textures = false

[header]
auto_load = true
level_name = single ; former level name, now can be mod name
level_ver = 1.0 ; former level version, now can be mod version
entry_point = $fs_root$\gamedata\ ; do not change !
creator = "Team EPIC" ; creator's name
link = "forum.epicstalker.com" ; creator's link